{
  "name": "server-arithmetic-calculator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "dist": "rimraf ./dist && tsc",
    "dev": "yarn dist && nodemon dist/index.js",
    "start": "node dist/index.js",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch --silent",
    "coverage": "jest --coverage",
    "test:e2e": "jest --config ./jest.e2e.config.js --verbose",
    "test:e2e:watch": "npm run test:e2e -- --watch --silent",
    "test:e2e:coverage": "npm run test:e2e -- --coverage --forceExit",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "lint:fix": "eslint src --fix",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "lint-staged": {
    "*.{js,json,css,scss,html,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "random-org": "^2.2.0",
    "sequelize": "^6.29.3",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^7.0.4",
    "nodemon": "^2.0.21",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "eslintIgnore": [
    "src/db/migrations/*"
  ]
}
